        <div class="row">
            <div class="col-md-4 left-column-layout-left">
                {% block contentLeftColumn %}{% endblock %}
            </div>
            <div class="col-md-4 left-column-layout-right">
                {% block contentCenterColumn %}{% endblock %}
            </div>
            <div class="col-md-4 left-column-layout-right">
                {% block contentRightColumn %}{% endblock %}
            </div>
        </div>
    </div>
{% endblock %}